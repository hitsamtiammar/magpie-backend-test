{
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  },
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true,
    "paths": {
      "prisma": ["./src/prisma"],
      "constant": ["./src/constant"],
      "middleware/*": ["./src/middleware/*"]
    }
  }
}
